<application>

  <name>SWoOz NAO connections/saver japan experience </name>
  <description>Save bottles containing SWoOz trackers data </description>

        <connection>
                <from>/tracking/rgbd/forest/head</from>
                <to>/manipulation/in0</to>
                <protocol>tcp</protocol>
        </connection>

        <connection>
                <from>/tracking/rgbd/emicp/head</from>
                <to>/manipulation/in1</to>
                <protocol>tcp</protocol>
        </connection>

        <connection>
                <from>/manipulation/out0</from>
                <to>/teleoperation/nao/head</to>
                <protocol>tcp</protocol>
        </connection>

        <connection>
                <from>/manipulation/out1</from>
                <to>/teleoperation/nao/head</to>
                <protocol>tcp</protocol>
        </connection>



        <connection>
                <from>/tracking/rgbd/forest/head</from>
                <to>/dump/swooz-forestHeadTracking</to>
                <protocol>tcp</protocol>
        </connection>

        <connection>
                <from>/tracking/rgbd/emicp/head</from>
                <to>/dump/swooz-emicpHeadTracking</to>
                <protocol>tcp</protocol>
        </connection>

        <connection>
                <from>/manipulation/out0</from>
                <to>/dump/swooz-forestHeadTracking-manipulated</to>
                <protocol>tcp</protocol>
        </connection>

        <connection>
                <from>/manipulation/out1</from>
                <to>/dump/swooz-emicpHeadTracking-manipulated</to>
                <protocol>tcp</protocol>
        </connection>



        <module>
                <name>dataDumper</name>
                <parameters>--rxTime --name /dump/swooz-emicpHeadTracking</parameters>
                <node>localhost</node>
                <tag>log</tag>
        </module>

        <module>
                <name>dataDumper</name>
                <parameters>--rxTime --name /dump/swooz-forestHeadTracking</parameters>
                <node>localhost</node>
                <tag>log</tag>
        </module>

        <module>
                <name>dataDumper</name>
                <parameters>--rxTime --name /dump/swooz-emicpHeadTracking-manipulated</parameters>
                <node>localhost</node>
                <tag>log</tag>
        </module>

        <module>
                <name>dataDumper</name>
                <parameters>--rxTime --name /dump/swooz-forestHeadTracking-manipulated</parameters>
                <node>localhost</node>
                <tag>log</tag>
        </module>




</application>

